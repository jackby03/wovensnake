{
    "name": "my-python-project",
    "version": "1.0.0",
    "description": "A Python project managed with WovenSnake",
    "main": "src/main.py",
    
    "dependencies": {
      "requests": "^2.26.0",
      "numpy": "^1.21.0"
    },
    
    "devDependencies": {
      "pytest": "^7.0.0",
      "black": "^22.0.0"
    },
  
    "envs": {
      "dev": {
        "python_version": "3.8",
        "extra_flags": ["--dev"]
      },
      "prod": {
        "python_version": "3.9",
        "extra_flags": ["--prod"]
      },
      "release": {
        "python_version": "3.10",
        "extra_flags": ["--release"]
      }
    },
  
    "workDirectories": {
      "dev": "venv-dev/",
      "prod": "venv-prod/",
      "release": "venv-release/"
    },
  
    "config": {
      "cache_dir": "./.woven_cache",
      "log_level": "info"
    }
  }
  